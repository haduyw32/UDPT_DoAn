<html>
<head>
	<meta charset="utf-8">
	<link href="css/default.css" rel="stylesheet" type="text/css">
	<link href="css/register.css" rel="stylesheet" type="text/css">
	<title>Home</title>
	<script src="js/jquery-1.9.1.min.js"></script>
	<script src="js/Cookie.js"></script>
	<% include 'header.ect' %>
</head>
<body>
	<% for table in @listTable : %>
	<div class="content">
		<table class="flatTable">
			<tr hidden>
				<td><%- table._id %></td>
			</tr>
			<tr class="titleTr">
				<td class="titleTd" colspan="4"><font size="6"><%- table.place.name %></font><p><%- table.place.address %></p></td>	
			</tr>
			<tr class="headingTr">
				<td>Loại Vacxin</td>
				<td>Tác Dụng</td>
				<td>Số mũi</td>
				<td>Ngày đăng kí</td>
			</tr>
			<% for vac in table.vacxin_docs : %>
			<tr>
				<td style="width: 13%"><%- vac._id %></td>
				<td style="width: 60%"><%- vac.use %></td>
				<td><%- vac.age.length %></td>
				<td class="controlTd">
					<div class="inputDate">
						<form>
							<input type="date">
						</form>
					</div>
				</td>
			</tr>
			<% end %>
		</table>
	</div>
	<% end %>
	
	<div class="footer">
		<form action="\register" method="post">
			<input type="submit" name="submit" value="Xác nhận" style="vertical-align:middle">
		</form>
	</div>
</body>
</html>